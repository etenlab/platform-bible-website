{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "deploy:dev": "cdk deploy -c env=dev devPlatformBibleApp",
    "deploy:prod": "cdk deploy -c env=prod prodPlatformBibleApp",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write \"{src,lib,config,bin}/**/*.{js,ts,json,yml,yaml}\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.4.2",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "aws-cdk": "2.88.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.88.0",
    "constructs": "^10.0.0",
    "js-yaml": "^4.1.0",
    "source-map-support": "^0.5.21"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint-fix"
    ]
  }
}
